<?xml version="1.0" encoding="UTF-8"?>
<Base:BasePage 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="XFArchitecture.Views.User.Register.RegisterPage"
    xmlns:Services="clr-namespace:XFArchitecture.Services"
    xmlns:Control="clr-namespace:XFArchitecture.Controls"
    xmlns:Base="clr-namespace:XFArchitecture.Views.Base"    
    Services:ViewModelLocator.AutoWireViewModel="True">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" Spacing="0" HeightRequest="-1" WidthRequest="-1" BackgroundColor="Transparent" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
            <Control:NavigationBar NavigationBarStyle="SearchBar" LeftCommand="{Binding GoBack}"/>
            <Entry Placeholder="&#xf0c7; Search" FontFamily="{StaticResource FARegular}"/>
        </StackLayout>
    </ContentPage.Content>
</Base:BasePage>