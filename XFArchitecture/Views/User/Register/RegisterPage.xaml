<?xml version="1.0" encoding="UTF-8"?>
<Base:BasePage 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="XFArchitecture.Views.User.Register.RegisterPage"
    xmlns:Services="clr-namespace:XFArchitecture.Services"
    xmlns:Control="clr-namespace:XFArchitecture.Controls"
    xmlns:Base="clr-namespace:XFArchitecture.Views.Base"    
    Services:ViewModelLocator.AutoWireViewModel="True">
    <ContentPage.Content>
        <StackLayout Orientation="Vertical" Spacing="0" HeightRequest="-1" WidthRequest="-1" BackgroundColor="Transparent" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
            <Control:NavigationBar NavigationBarStyle="CustomBar" LeftCommand="{Binding GoBack}" HasTitle="true" HasSubtitle="false" HasTitleIcon="true" HasBackButton="true" HasRightButtons="Two"
                                   TitleIcon="ic_checkbox_checked" SubtitleText="Hello World!" LeftIcon="{StaticResource Menu}" RightTwoIcon="{StaticResource Search}" RightOneIcon="{StaticResource Save}"/>
        </StackLayout>
    </ContentPage.Content>
</Base:BasePage>